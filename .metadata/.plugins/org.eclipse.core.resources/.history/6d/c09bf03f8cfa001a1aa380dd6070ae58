package oo.day05;

import oo.day04.Doctor;
import oo.day04.Person;
import oo.day04.Student;
import oo.day04.Teacher;

public class Test {
	public static void main(String[] args) {
		Person[] ps = new Person[5];
		ps[0] = new Student("zhangsan", 16, "LF", "111");
		ps[1] = new Student("lisi", 17, "CS", "222");
		ps[2] = new Teacher("wangwu", 26, "NC", 8000.0);
		ps[3] = new Teacher("zhaoliu", 25, "BJ", 9000.0);
		ps[4] = new Doctor("sunqi", 36, "CQ", "主任医师");
		for (int i = 0; i < ps.length; i++) {
			System.out.println(ps[i].name);
			ps[i].sayHi();
		}
	}
}
